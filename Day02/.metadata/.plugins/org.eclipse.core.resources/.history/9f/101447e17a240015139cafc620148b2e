package com.hfut.whiledemo;

public class Demo {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
//    System.out.println(1);
//    System.out.println(2);
    
    int i=1;
    while(i<=100){
    	 System.out.println(i);	
    	 i++;
    }
    
    System.out.println("跳出循环后i的值："+i);
   
	}

}
