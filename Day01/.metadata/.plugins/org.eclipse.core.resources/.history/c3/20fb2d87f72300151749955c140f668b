package com.hfut.demo;

public class Demo {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
    float i=5.0f;
    int j=3;
//    System.out.println("i+j="+(i+j));
//    System.out.println("i/j="+(i/j));
//    System.out.println("i%j="+(i%j));
	//前缀递增
    System.out.println(++i);
    //后缀递增
    System.out.println(j++);
	}

}
 